{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "vercel_app.py"
    },
    {
      "src": "/(.*)",
      "dest": "vercel_app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "CORS_ORIGINS": "https://ai-meetingnotes.vercel.app,https://hybrid-transcription.vercel.app"
  },
  "functions": {
    "vercel_app.py": {
      "maxDuration": 30
    }
  }
}
