{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "CORS_ORIGINS": "https://ai-meetingnotes.vercel.app"
  },
  "functions": {
    "app.py": {
      "runtime": "@vercel/python",
      "maxDuration": 30,
      "includeFiles": "**"
    }
  }
}
